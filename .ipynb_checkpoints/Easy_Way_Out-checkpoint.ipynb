   "outputs": [],
   "source": [
    "from scipy.io.harwell_boeing import hb_read, hb_write\n",
    "from scipy.sparse import csr_matrix, eye"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "db58836c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "  (0, 0)\t1.0\n",
      "  (1, 1)\t1.0\n",
      "  (2, 2)\t1.0\n"
     ]
    }
   ],
   "source": [
    "data = csr_matrix(eye(3))  # create a sparse matrix\n",
    "hb_write(\"data.hb\", data)  # write a hb file\n",
    "print(hb_read(\"data.hb\"))  # read a hb file"
   ]
  },
